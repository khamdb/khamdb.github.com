<VideoAdServingTemplate xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="vast.xsd">
      <Ad id="preroll">
          <Wrapper>
             <AdSystem>MyAdSystem</AdSystem>
                <VASTAdTagURL>
                    <URL><![CDATA[http://static.openvideoads.org/tests/vast2-wrapper-to-vast1-ad.xml]]></URL>
                </VASTAdTagURL>
                <Impression>
                   <URL><![CDATA[http://localhost/vast1-wrapper2-impression]]></URL>
                </Impression>
                <Error>
                   <URL><![CDATA[http://localhost/vast1-wrapper2-error]]></URL>
                </Error>
				<VideoClicks>
					<ClickTracking>
						<URL id="anotheradserver"><![CDATA[http://localhost/vast1-wrapper2-clicktracking-1]]></URL>
						<URL id="athirdadserver"><![CDATA[http://localhost/vast1-wrapper2-clicktracking-2]]></URL>
					</ClickTracking>
				</VideoClicks>
                <TrackingEvents>
                    <Tracking event="start">
                        <URL id="primaryAdServer"><![CDATA[http://localhost/vast1-wrapper2-start]]></URL>
                    </Tracking>
                    <Tracking event="midpoint">
                        <URL id="primaryAdServer"><![CDATA[http://localhost/vast1-wrapper2-midpoint]]></URL>
                    </Tracking>
                    <Tracking event="firstQuartile">
                        <URL id="primaryAdServer"><![CDATA[http://localhost/vast1-wrapper2-firstQuartile]]></URL>
                    </Tracking>
                    <Tracking event="thirdQuartile">
                        <URL id="primaryAdServer"><![CDATA[http://localhost/vast1-wrapper2-thirdQuartile]]></URL>
                    </Tracking>
                    <Tracking event="complete">
                        <URL id="primaryAdServer"><![CDATA[http://localhost/vast1-wrapper2-complete]]></URL>
                    </Tracking>
                    <Tracking event="mute">
                        <URL id="primaryAdServer"><![CDATA[http://localhost/vast1-wrapper2-mute]]></URL>
                    </Tracking>
                    <Tracking event="replay">
                        <URL id="primaryAdServer"><![CDATA[http://localhost/vast1-wrapper2-replay]]></URL>
                    </Tracking>
                    <Tracking event="fullscreen">
                        <URL id="primaryAdServer"><![CDATA[http://localhost/vast1-wrapper2-fullscreen]]></URL>
                    </Tracking>
                    <Tracking event="stop">
                        <URL id="primaryAdServer"><![CDATA[http://localhost/vast1-wrapper2-stop]]></URL>
                    </Tracking>
                    <Tracking event="unmute">
                        <URL id="primaryAdServer"><![CDATA[http://localhost/vast1-wrapper2-unmute]]></URL>
                    </Tracking>
                    <Tracking event="pause">
                        <URL id="primaryAdServer"><![CDATA[http://localhost/vast1-wrapper2-pause]]></URL>
                    </Tracking>
                    <Tracking event="resume">
                        <URL id="primaryAdServer"><![CDATA[http://localhost/vast1-wrapper2-resume]]></URL>
                    </Tracking>
                </TrackingEvents>
          </Wrapper>
      </Ad>
</VideoAdServingTemplate>